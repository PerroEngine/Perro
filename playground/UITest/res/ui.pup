@script UIScript extends UINode

on init() {
    Console.print("ui script init")
}


on main_button_Pressed() {
    Console.print_info("===================> main button pressed")
}

on main_button_Hovered() {
    Console.print_info("===================> main button hovered")
}

on main_button_Unhovered() {
    Console.print_info("===================> main button not hovered")
}

on main_button_Released() {
    Console.print_info("===================> main button released")
}

