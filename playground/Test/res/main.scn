@root = main

[main]
[Node3D]
    position = (0, 0, 0)
[/Node3D]
[/main]

[cube]
parent = @root
script = "res://scripts/script.rs"

[MeshInstance3D]
    mesh = "__cylinder__"

    [Node3D]
        position = (0, 0, 0)
    [/Node3D]
[/MeshInstance3D]
[/cube]

[camera]
parent = @root

[Camera3D]
    active = true
    zoom = 1.0

    [Node3D]
        position = (0, 0, 5)
    [/Node3D]
[/Camera3D]
[/camera]

[sun]
parent = @root

[RayLight3D]
    color = (1.0, 0.98, 0.92)
    intensity = 0.9
    active = true

    [Node3D]
        position = (0, 0, 0)
    [/Node3D]
[/RayLight3D]
[/sun]

