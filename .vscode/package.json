{
	"name": "pup-language",
	"displayName": "Pup Language Support",
	"description": "Syntax highlighting for .pup files",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.0.0"
	},
	"contributes": {
		"languages": [
			{
				"id": "pup",
				"aliases": ["Pup", "pup"],
				"extensions": [".pup"],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "pup",
				"scopeName": "source.pup",
				"path": "./pup.tmLanguage.json"
			}
		]
	}
}
